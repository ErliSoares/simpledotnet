<?xml version="1.0" encoding="utf-8" ?>
<basicLibrary>
  <threading>
    <defaultLockingProvider>
      <connectionStringName>PersistenceCS</connectionStringName>
      <tableName>t_acn_instance_state</tableName>
      <typeColumn>class_name</typeColumn>
      <idColumn>id_order_block</idColumn>
      <semaphoreColumn>semaphore</semaphoreColumn>
      <data>data</data>
      <secondsToWait>10</secondsToWait>
    </defaultLockingProvider>
  </threading>
  <log4net>
    <appender name="_DefaultLogFileAppender" type="log4net.Appender.FileAppender">
      <file value="\MetraTech\RMP\OMLog.txt" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%class][%level] - %message%newline" />
      </layout>
    </appender>

    <logger name="_DefaultLog">
      <level value="ALL"/>
      <appender-ref ref="_DefaultLogFileAppender" />
    </logger>

    <root>
    </root>
  </log4net>
</basicLibrary>